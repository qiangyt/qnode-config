{
    "name": "node-config-any",
    "version": "1.0.0",
    "description": "Node.js configuration file loader. Support YAML, JSON and plain javascript. HTTP(s) & git support is planned too",
    "main": "src/ConfigHelper.js",
    "scripts": {
        "coverage": "JASMINE_CONFIG_PATH=./jasmine.json node ./node_modules/istanbul/lib/cli.js cover -color ./node_modules/jasmine/bin/jasmine.js",
        "test": "JASMINE_CONFIG_PATH=./jasmine.json node ./node_modules/jasmine/bin/jasmine.js",
        "lint": "./node_modules/.bin/eslint -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/",
        "watch": "./node_modules/.bin/esw --watch -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/"
    },
    "repository": "git@github.com:qiangyt/node-config-any.git",
    "author": "Qiang Yiting <qiangyt@wxcount.com> (https://github.com/qiangyt)",
    "keywords": [
        "yaml",
        "node.js",
        "configuration",
        "profile",
        "json"
    ],
    "bugs": {
        "url": "https://github.com/qiangyt/node-config-any/issues"
    },
    "engines": {
        "node": ">=6"
    },
    "private": false,
    "license": "MIT",
    "dependencies": {
        "js-yaml": "3.9.1",
        "json-log4js": "1.0.0",
        "lodash": "4.17.4"
    },
    "devDependencies": {
        "babel": "6.23.0",
        "babel-cli": "6.24.1",
        "babel-eslint": "7.2.3",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "eslint": "4.3.0",
        "eslint-stylish": "0.2.0",
        "eslint-watch": "3.1.2",
        "istanbul": "1.1.0-alpha.1",
        "jasmine": "2.7.0",
        "mock-fs": "4.4.1",
        "mock-require": "2.0.2",
        "sinon": "2.4.1"
    },
    "browser": {
        "os": false
    }
}
